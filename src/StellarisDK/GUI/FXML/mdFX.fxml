<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="main" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <GridPane fx:id="grid" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="60.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="120.0" />
            <RowConstraints minHeight="10.0" />
        </rowConstraints>
         <children>
            <TextField id="name" fx:id="name" GridPane.columnIndex="1" />
            <TextField id="path" fx:id="path" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField id="archive" fx:id="archive" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <ListView id="dependencies" fx:id="dependencies" editable="true" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <TextField id="picture" fx:id="picture" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <TextField id="remote_file_id" fx:id="remote_file_id" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <TextField id="supported_version" fx:id="supported_version" GridPane.columnIndex="1" GridPane.rowIndex="6" />
            <ListView id="tags" fx:id="tags" editable="true" GridPane.columnIndex="1" GridPane.rowIndex="7" />
            <Label text="Mod Name" />
            <Label text="Path" GridPane.rowIndex="1" />
            <Label text="Archive" GridPane.rowIndex="2" />
            <Label text="Dependencies" GridPane.rowIndex="3" GridPane.valignment="TOP" />
            <Label text="Picture" GridPane.rowIndex="4" />
            <Label text="Remote File ID" GridPane.rowIndex="5" />
            <Label text="Game Version" GridPane.rowIndex="6" />
            <Label alignment="TOP_LEFT" text="Tags" GridPane.rowIndex="7" GridPane.valignment="TOP" />
            <Button id="btn_save" fx:id="btn_save" mnemonicParsing="false" text="Save" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="8">
               <GridPane.margin>
                  <Insets bottom="5.0" top="5.0" />
               </GridPane.margin>
            </Button>
         </children>
         <padding>
            <Insets left="5.0" right="5.0" top="5.0" />
         </padding>
      </GridPane>
   </children>
</AnchorPane>
