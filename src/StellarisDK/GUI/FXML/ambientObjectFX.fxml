<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="main" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <GridPane layoutX="10.0" layoutY="10.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="110.0" />
                <ColumnConstraints hgrow="SOMETIMES" />
                <ColumnConstraints hgrow="SOMETIMES" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <padding>
                <Insets bottom="40.0" />
            </padding>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Label text="Name" />
                <Label text="Entity" GridPane.rowIndex="1" />
                <Label text="Tooltip" GridPane.rowIndex="2" />
                <TextField id="name" fx:id="name" GridPane.columnIndex="2" />
                <TextField id="entity" fx:id="entity" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                <TextField id="tooltip" fx:id="tooltip" GridPane.columnIndex="2" GridPane.rowIndex="2" />
            <Label text="Description" GridPane.rowIndex="4" />
            <TextField id="description" fx:id="description" GridPane.columnIndex="2" GridPane.rowIndex="4" />
            <CheckBox id="show_name" fx:id="show_name" mnemonicParsing="false" text="Show Name" GridPane.columnIndex="3" />
            <CheckBox id="selectable" fx:id="selectable" mnemonicParsing="false" text="Selectable" GridPane.columnIndex="3" GridPane.rowIndex="1" />
              <Button id="btn_save" fx:id="btn_save" mnemonicParsing="false" text="Save" GridPane.columnIndex="3" GridPane.halignment="RIGHT" GridPane.rowIndex="5" />
            </children>
        </GridPane>
    </children>
</AnchorPane>
